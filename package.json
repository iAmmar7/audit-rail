{
  "name": "audit-rail-client",
  "version": "4.3.0",
  "private": true,
  "description": "Ensuring Excellence, One Station at a Time",
  "author": "Ammar Ansari",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=en-US --write",
    "postinstall": "umi g tmp",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.3",
    "@ant-design/icons": "^4.8.3",
    "@ant-design/pro-card": "^1.20.22",
    "@ant-design/pro-descriptions": "^1.12.7",
    "@ant-design/pro-form": "^1.74.7",
    "@ant-design/pro-layout": "^6.38.22",
    "@ant-design/pro-table": "^2.80.8",
    "@umijs/route-utils": "^1.0.37",
    "antd": "^4.24.16",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^16.14.0",
    "react-csv": "^2.2.2",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.3.0",
    "umi": "^3.5.41",
    "umi-request": "^1.4.0",
    "use-merge-value": "^1.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.28",
    "@umijs/fabric": "^4.0.1",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.32",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^7.32.0",
    "jsdom-global": "^3.0.2",
    "mockjs": "^1.0.1-beta3",
    "puppeteer-core": "^5.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
